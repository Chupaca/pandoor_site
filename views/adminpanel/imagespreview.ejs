
    <% images.forEach((image, i) => { %>
    <div class="gallery_image" data-imageid="<%= image.id %>">
        <img class="image_one" data-imageid="<%= image.id %>" data-index="<%= i %>" src="https://storage.googleapis.com/<%= image.metadata.bucket %>/<%= image.id %>" alt="">
        <div class="desc">
                <span><%= image.id.slice(14) %></span>
            <i class="fas fa-trash-alt <%= AllowRemove?'':'remove_file'%>" onclick="<%= AllowRemove?'RemoveFile.call(this)':''%>"  data-imageid="<%= image.id %>" data-bucket="<%= bucketFile || '' %>"></i>
        </div>
    </div>
    <%})%>